
/* @import '../../widgets/modules/modules.wxss'; */
.view-mine{
	background-color: #f9f9f9;
	min-height: 100%
}

.view-mine .pannel{
	margin-bottom:20rpx;
	background-color: white;
	padding: 0 0 40rpx 0;
}

.view-mine .pannel .header .title {
	padding : 0 20rpx;

}

.view-mine .block { height : 120rpx; }

.view-mine .user-info {
	position: fixed; 
	top : 0;
	left : 0;
	z-index: 100;
	width : 100%;
	display: flex;
	justify-content: space-around;
	color : #fff;
	font-size : 24rpx;
	padding-top : 20rpx;
	padding-bottom : 40rpx;
	background-image:linear-gradient(0deg,#da3633,#e05e39);
}

.view-mine .user-info .mod {
	display: flex;
	justify-content: center;
	flex-direction: column;
	align-items: center;
}

.view-mine .user-info .act-mod .text {
	font-size : 26rpx;
}

.view-mine .user-info .act-mod .point {
	font-size : 32rpx;
	color : #f0b41b;
	margin-top : 10rpx;
}

.view-mine .user-info .act-mod .button {
	margin-top : 18rpx;
	border : solid 1px rgba( 255, 255, 255, 0.7 );
	height : 40rpx;
	width : 100rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	border-radius : 20rpx;
	background-color: rgba( 0, 0, 0, 0.1 )
}

.view-mine .user-info .logo .icon {
	position: relative;
}

.view-mine .user-info .logo .icon image {
	width : 140rpx;
	height: 140rpx;
	border-radius: 50%;
	border : solid 1px #fff;
}

.view-mine .user-info .logo .name {
	font-size : 32rpx;
	margin-top : 10rpx;
}

.view-mine .user-info .level {
	display: flex; 
	align-items: center; 
	padding : 6rpx 20rpx;
	z-index: 10;
	color : #f0b41b;
}
.view-mine .user-info .level image {
	border : none;
	border-radius: 0;
	width : 30rpx;
	height : 30rpx;
	margin-right : 8rpx;
}


.view-mine .user{
	display:flex;
height:140rpx;
position:relative;
width:710rpx;

/*justify-content:space-between;*/
/*padding:44rpx;*/
}
.view-mine .user .charge{
	width:50rpx;
	height:50rpx;
	position:absolute;
	right:0;
	bottom:0.5px;
}

.view-mine .user .left{
	margin-right:30rpx;
	width:20%;

}
.view-mine .user .right{
	position:relative;
	width:80%;
}
.view-mine .user .vip-wrapper{
	margin-top:6rpx;
	position:absolute;
	width:300rpx;
	bottom:0;

}
.view-mine .user .name{
	font-size:40rpx;
	height:40rpx;
	line-height:40rpx;
}
.view-mine .user .vip{
	width:30rpx;
	height:30rpx;
	/*margin-left:10rpx;*/
	vertical-align:bottom;
}
.view-mine .user .vip-level{
	vertical-align:bottom;
	color:#f0b41b;
	font-size:30rpx;
	position:absolute;
	bottom:-8rpx;

}
.view-mine .user .notice{
	position:absolute;
	bottom:0;
}
.view-mine .user .check-user{
	font-size:24rpx;
color:#a6a6a6;
height:28rpx;
line-height:28rpx
}
.view-mine .user .header-img{
	width:140rpx;
height:140rpx;
border-radius:50%;
}


