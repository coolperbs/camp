}
.page-childlist .child-record{
    position: relative;

}
.page-childlist .child-record .line{
    display: flex;
    align-items: center;
    height: 30rpx;
    padding: 20rpx 40rpx;

}
.page-childlist .add-child{
    position: fixed;
    bottom: 0;
    background-color: white;
    width:100%;
    height: 100rpx;
    /* border-top: 1px solid #f0f0f0; */
    display: flex;
    justify-content: center;
    align-items: center;
}
.page-childlist .add-child .btn{
    width: 700rpx;
    height: 80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #8bc34a;
    border-radius:8rpx;
    color: white
}
.page-childlist .child-record .field-logo{
    height: 30rpx;
    width: 30rpx;
    margin-right: 10rpx;
}
.page-childlist .add-child .add-logo{
    height: 30rpx;
    width: 30rpx;
    margin-right: 20rpx;
}
.page-childlist .edit-logo{
    height:50rpx;
    width:50rpx;
    position:absolute;
